# Loading work.master_e(master_a)#1
add wave -position end  sim:/master_tb/clk_i
add wave -position end  sim:/master_tb/rst_i
add wave -position end  sim:/master_tb/rx_i
add wave -position end  sim:/master_tb/sw1
add wave -position end  sim:/master_tb/sw2
add wave -position end  sim:/master_tb/l_o
add wave -position end  sim:/master_tb/pas_o

run 50 ms
